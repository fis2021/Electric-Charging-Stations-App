<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fis2021.controllers.StatisticReportController">
    <children>
        <Label alignment="CENTER" prefHeight="90.0" prefWidth="400.0" style="-fx-background-color: #c690c6; -fx-border-color: black;" text="Statistic Report">
            <font>
                <Font name="System Bold" size="22.0" />
            </font>
        </Label>
        <Button id="backButton" fx:id="backButton" layoutY="349.0" mnemonicParsing="false" onAction="#backButtonOnAction" prefHeight="50.0" prefWidth="400.0" style="-fx-background-color: #ffcc00; -fx-border-color: #ff9900;" text="Back">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <Button id="okButton" fx:id="okButton" layoutX="296.0" layoutY="164.0" mnemonicParsing="false" onAction="#okButtonOnAction" prefHeight="25.0" prefWidth="38.0" style="-fx-background-color: #00FF00; -fx-border-color: green;" text="OK" />
        <Label fx:id="selectDateLabel" alignment="CENTER" layoutX="13.0" layoutY="201.0" prefHeight="25.0" prefWidth="375.0" textFill="RED">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
      <Label alignment="CENTER" layoutX="54.0" layoutY="165.0" prefHeight="25.0" prefWidth="44.0" text="Date">
         <font>
            <Font size="15.0" />
         </font></Label>
      <DatePicker fx:id="datePicker" editable="false" layoutX="106.0" layoutY="165.0" showWeekNumbers="true" />
    </children>
</AnchorPane>
