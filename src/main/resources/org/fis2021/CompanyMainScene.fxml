<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Ellipse?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fis2021.controllers.CompanyHomeController">
   <children>
      <AnchorPane prefHeight="100.0" prefWidth="800.0" style="-fx-background-color: #c690c6;">
         <children>
            <Ellipse fill="#ffcc00" layoutX="150.0" layoutY="50.0" radiusX="50.0" radiusY="40.0" stroke="BLACK" strokeType="INSIDE" />
            <Ellipse fill="#ffcc00" layoutX="400.0" layoutY="50.0" radiusX="50.0" radiusY="40.0" stroke="BLACK" strokeType="INSIDE" />
            <Button fx:id="overviewButton" layoutX="118.0" layoutY="19.0" mnemonicParsing="false" onAction="#handleButtonActionOverview" prefHeight="61.0" prefWidth="63.0" style="-fx-background-color: trasparent;" text="O">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="popularityChartButton" layoutX="368.0" layoutY="19.0" mnemonicParsing="false" onAction="#handlePopularityChartButtonOnAction" prefHeight="61.0" prefWidth="63.0" style="-fx-background-color: trasparent;" text="P">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <Button fx:id="newStationButton" layoutY="100.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="100.0" prefWidth="800.0" style="-fx-background-color: #00FF00;" text="+ NEW STATION">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <ListView fx:id="listView" layoutY="200.0" prefHeight="500.0" prefWidth="800.0" />
      <Ellipse fill="#ffcc00" layoutX="650.0" layoutY="50.0" radiusX="50.0" radiusY="40.0" stroke="BLACK" strokeType="INSIDE" />
      <Button fx:id="logoutButton" layoutX="618.0" layoutY="19.0" mnemonicParsing="false" onAction="#logoutButtonOnAction" prefHeight="61.0" prefWidth="63.0" style="-fx-background-color: trasparent;" text="L">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
